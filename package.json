{
  "name": "template-react14",
  "version": "1.0.0",
  "description": "微前端子项目模板-适配React14（IE8）",
  "private": true,
  "license": "MIT",
  "scripts": {
    "start": "npm run dev",
    "dev": "cross-env NODE_ENV=development BUILD_ENV=dev webpack-dev-server --config webpack.dev.js --open",
    "dev:ie8": "cross-env NODE_ENV=production BUILD_ENV=dev webpack-dev-server --config webpack.prod.js --open",
    "build": "cross-env NODE_ENV=production BUILD_ENV=prod webpack --config webpack.prod.js",
    "build:sit": "cross-env NODE_ENV=production BUILD_ENV=sit webpack --config webpack.prod.js",
    "build:pre": "cross-env NODE_ENV=production BUILD_ENV=pre webpack --config webpack.prod.js",
    "build:prod": "cross-env NODE_ENV=production BUILD_ENV=prod webpack --config webpack.prod.js",
    "build:mock": "cross-env NODE_ENV=production BUILD_ENV=mock webpack --config webpack.prod.js",
    "analyse": "cross-env NODE_ENV=production ANALYSE=1 webpack --config webpack.prod.js",
    "dll": "cross-env NODE_ENV=production webpack --config webpack.dll.js",
    "dll:dev": "cross-env NODE_ENV=development webpack --config webpack.dll.js",
    "lint": "npx eslint ./src --fix"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "src/**/*.{js,jsx}": [
      "eslint --fix"
    ]
  },
  "devDependencies": {
    "autoprefixer": "^9.7.6",
    "babel-core": "6.26.3",
    "babel-eslint": "8.2.6",
    "babel-loader": "6.4.1",
    "babel-plugin-import": "1.13.0",
    "babel-plugin-transform-decorators-legacy": "1.3.5",
    "babel-plugin-transform-flow-strip-types": "6.22.0",
    "babel-plugin-transform-regenerator": "6.26.0",
    "babel-plugin-transform-runtime": "6.23.0",
    "babel-plugin-webpack-dynamic-import": "1.0.1",
    "babel-preset-env": "1.7.0",
    "babel-preset-react": "6.24.1",
    "babel-preset-stage-2": "6.24.1",
    "clean-webpack-plugin": "^3.0.0",
    "copy-webpack-plugin": "4.6.0",
    "cross-env": "^7.0.2",
    "css-loader": "0.26.4",
    "es3ify-loader": "^0.2.0",
    "es3ify-webpack-plugin": "^0.1.0",
    "eslint": "^6.8.0",
    "eslint-config-airbnb": "^18.1.0",
    "eslint-config-prettier": "^6.7.0",
    "eslint-config-react-important-stuff": "^2.0.0",
    "eslint-import-resolver-alias": "^1.1.2",
    "eslint-import-resolver-webpack": "^0.12.2",
    "eslint-loader": "^3.0.3",
    "eslint-plugin-import": "^2.20.1",
    "eslint-plugin-jest": "^23.19.0",
    "eslint-plugin-jsx-a11y": "^6.2.3",
    "eslint-plugin-prettier": "^3.1.1",
    "eslint-plugin-react": "^7.19.0",
    "eslint-plugin-react-hooks": "^2.5.1",
    "file-loader": "^6.0.0",
    "html-webpack-plugin": "^3.2.0",
    "husky": "^4.2.5",
    "lint-staged": "^10.2.11",
    "mini-css-extract-plugin": "^0.9.0",
    "object-defineproperty-ie8": "^1.0.1",
    "optimize-css-assets-webpack-plugin": "^5.0.3",
    "terser-webpack-plugin": "^3.0.7",
    "uglifyjs-webpack-plugin": "^2.2.0",
    "webpack": "1.15.0",
    "webpack-cli": "1.5.3",
    "webpack-dev-server": "1.15.0",
    "node-sass": "^4.8.3",
    "postcss-loader": "3.0.0",
    "less-loader": "2.2.3",
    "sass-loader": "6.0.7",
    "style-loader": "0.13.2",
    "url-loader": "0.5.9",
    "webpack-merge": "^4.2.2"
  },
  "dependencies": {
    "antd": "^1.11.6",
    "axios": "0.19.0",
    "core-js": "2",
    "history": "^2.1.2",
    "js-cookie": "^2.2.1",
    "jsonp": "^0.2.1",
    "object-assign": "^4.1.1",
    "react": "^0.14.9",
    "react-deliverer": "^3.1.25",
    "react-dom": "^0.14.9",
    "react-redux": "5.0.1",
    "redux": "3.7.2",
    "redux-thunk": "2.0.1"
  }
}
